{
  "name": "examplify",
  "version": "1.0.0",
  "description": "Examplify mission is make online exam/ testing easy for both exam taker and giver.",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html",
    "clean": "rm -rf build/ && rm -rf dist/"
  },
  "repository": {
